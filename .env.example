# Port to expose FileBrowser on host
FILEBROWSER_PORT=50000

# User and group IDs for file permissions
PUID=1000
PGID=1000

# FileBrowser initial login credentials
# NOTE: These are only used for initial setup when filebrowser.db doesn't exist
# After the database is created, credentials must be changed through the web interface
INITIAL_USERNAME=admin
INITIAL_PASSWORD=password